{
  "name": "grpc-playground-common",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "https://github.com/esmail-elmoussel/grpc-playground.git",
  "author": "Esmail Elmoussel <elmoussel12@gmail.com>",
  "scripts": {
    "build": "mkdir -p build && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./build --grpc_out=grpc_js:./build --plugin=protoc-gen-grpc=/usr/local/lib/node_modules/grpc-tools/bin/grpc_node_plugin -I ./protos ./protos/*.proto && protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./build -I ./protos ./protos/*.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.1",
    "google-protobuf": "^3.21.2"
  },
  "devDependencies": {
    "grpc_tools_node_protoc_ts": "^5.3.3"
  }
}
